{
  "name": "game-manager-v2",
  "version": "0.0.1",
  "description": "Version 2 of the popular and groundbreaking FIFA app 'Game Manager'",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon ./server/index.js",
    "start": "node ./server/index.js && npm run build",
    "dev": "webpack-dev-server --config webpack.config.js --color --progress --watch",
    "build": "npm run clean && webpack",
    "clean": "rimraf ./public",
    "test": "npm run test:server && npm run test:client",
    "test:server": "./test/run.sh",
    "test:client": "jest",
    "client:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlaMurkan-FIFA-Boyz-2017-Dremz2Lyf/Game-Manager-V2.git"
  },
  "keywords": [
    "react",
    "express",
    "webpack",
    "redux",
    "postgresql",
    "jest",
    "enzyme",
    "mocha",
    "chai"
  ],
  "author": "Scott Charles Harris Horn <scotthorn0@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlaMurkan-FIFA-Boyz-2017-Dremz2Lyf/Game-Manager-V2/issues"
  },
  "homepage": "https://github.com/AlaMurkan-FIFA-Boyz-2017-Dremz2Lyf/Game-Manager-V2#readme",
  "devDependencies": {
    "axios-mock-adapter": "^1.7.1",
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.22.0",
    "bluebird": "^3.4.7",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.12.2",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^18.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-router": "^3.0.2",
    "react-test-renderer": "^15.4.2",
    "redux-form": "^6.5.0",
    "redux-logger": "^2.8.1",
    "redux-mock-store": "^1.2.2",
    "redux-thunk": "^2.2.0",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "supertest": "^2.0.1",
    "webpack": "^2.2.0-rc.0",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "knex": "^0.12.6",
    "path": "^0.12.7",
    "pg": "^6.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "rimraf": "^2.6.1"
  },
  "jest": {
    "setupFiles": [
      "./client_spec/jestsetup.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/jestsetup.js",
      "/mockData.js"
    ],
    "snapshotSerializers": [
      "./node_modules/enzyme-to-json/serializer"
    ]
  },
  "nyc": {
    "exclude": [
      "*/db.js",
      "*/test-helper.js",
      "*/test/"
    ],
    "report-dir": "./servCoverage"
  }
}
